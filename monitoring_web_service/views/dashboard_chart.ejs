<body class="jui">
  <div class="popover top">
    <div class="head">Title</div>
    <div class="body">
      <div class="image"><i class="icon-report"></i></div>
      <div class="message"></div>
    </div>
  </div>
  <div class="container">
    <nav id="navbar" class="navbar navbar-inverse" role="navigation">
      <div class="container-fluid">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse-1">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="">System Monitoring</a>
        </div>

      </div>
      <div class="navbar-collapse collapse" id="navbar-collapse-1">
        <ul class="nav navbar-nav">
          <li id="nav_list_1" class=""><a href="#os_list" onClick="call_page('os_list')">OS-List</a></li>
          <li id="nav_list_2" class="active"><a href="#dashboard" onClick="call_page('dashboard')">Dashboard</a></li>
          <li id="nav_list_3" class=""><a href="#cpu_chart" onClick="call_page('cpu_chart')">CPU</a></li>
          <li id="nav_list_4" class=""><a href="#cpu_core_chart" onClick="call_page('cpu_core_chart')">CPU Core</a></li>
          <li id="nav_list_5" class=""><a href="#memory_chart" onClick="call_page('memory_chart')">Memory</a></li>
          <li id="nav_list_6" class=""><a href="#processes_chart" onClick="call_page('processes_chart')">Processes</a></li>
          <li id="nav_list_7" class=""><a href="#setting" onClick="call_page('setting')">Setting</a></li>
          <li id="nav_list_8" class=""><a href="./download" >download</a></li>
        </ul>
      </div>
    </nav>
    <div class="my_dashboard_container">
      <div class="dashboard_chart_area">
        <div class="block cpu_chart_area">
          <div class="block chart realtime_cpu" id="realtime_cpu_chart">


          </div>
          <div class="block chart avg_cpu" id="avg_cpu_chart">

          </div>
        </div>
        <div class="block memory_chart_area">
          <div class="block chart realtime_memory" id="realtime_memory_chart">

          </div>
          <div class="block chart avg_memory" id="avg_memory_chart">


          </div>
        </div>
        <div class="block core_chart_area">

        </div>
        <div class="block process_chart_area">
          <div class="block chart process_cpu" id="process_cpu_chart">


          </div>
          <div class="block chart process_memory" id="process_memory_chart">


          </div>
          <div class="block chart process_thread" id="process_thread_chart">

          </div>
        </div>
      </div>
      <div class="dashboard_table_area">
        <div id="process_info_table" class="xtable" style="margin-top: 7px;">
          <table class="table simple headline nowrap">
            <thead>
              <tr>
                <th>Name</th>
                <th>ID</th>
                <th>CPU</th>
                <th>Memory</th>
                <th>Thread Count</th>
              </tr>
            </thead>
            <tbody></tbody>
          </table>
        </div>
        <script type="text/template" id="tpl_row">
          <! if(selectedProcessRowName == process_id) { !>
            <tr class="selected">
          <! } else { !>
            <tr>
          <! } !>
            <td>
              <!= name !>
            </td>
            <td>
              <!= process_id !>
            </td>
            <td>
              <!= process_cpu !> %
            </td>
            <td>
              <!= process_memory !> MB
            </td>
            <td>
              <!= process_thread !>
            </td>
          </tr>
        </script>
        <script id="tpl_none" type="text/template">
          <tr>
            <td colspan="5" class="none" align="center">Data does not exist.</td>
          </tr>
        </script>
      </div>

    </div>

  </div>
</body>

</html>
