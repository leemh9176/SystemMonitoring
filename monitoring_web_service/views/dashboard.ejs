<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <script type="text/javascript" src="javascripts/jquery.min.js"></script>
  <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
  <link rel="stylesheet" href="stylesheets/style.css">
  <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
  <script src="http://code.jquery.com/jquery-latest.min.js"></script>
  <!-- Basic style components -->
  <link rel="stylesheet" href="/jui/dist/ui.min.css" />
  <link rel="stylesheet" href="/jui/dist/ui-jennifer.min.css" />
  <!-- Grid style components -->
  <link rel="stylesheet" href="/jui-grid/dist/grid.min.css" />
  <link rel="stylesheet" href="/jui-grid/dist/grid-jennifer.min.css" />
  <script type="text/javascript" src="javascripts/function.js"></script>
  <script type="text/javascript" src="/jui-core/dist/core.min.js"></script>
  <script type="text/javascript" src="/jui/dist/ui.min.js"></script>
  <script type="text/javascript" src="/jui-grid/dist/grid.min.js"></script>
  <script type="text/javascript" src="/jui-chart/dist/chart.min.js"></script>
  <!-- <script type="text/javascript" src="javascripts/jui_control.js"></script> -->
  <script type="text/javascript" src="/javascripts/dashboard_control.js"></script>
  <script type="text/javascript">
    function callResize() {
      var height = document.body.scrollHeight;
      console.log(height);
      parent.Resize_Iframe(height);
    }
  </script>
  <title>System Monitoring Service</title>
</head>

<body class="jui">
  <div class="popover top">
    <div class="head">Title</div>
    <div class="body">
      <div class="image"><i class="icon-report"></i></div>
      <div class="message"></div>
    </div>
  </div>
  <div class="my_dashboard_container">
    <div class="dashboard_chart_area">
      <div class="block cpu_chart_area">
        <h4>CPU Usage Chart</h4>

        <div class="block chart realtime_cpu" id="realtime_cpu_chart">

        </div>
        <div class="block chart avg_cpu" id="avg_cpu_chart">

        </div>
      </div>
      <div class="block memory_chart_area">
        <h4>Memory Usage Chart</h4>
        <div class="block chart realtime_memory" id="realtime_memory_chart">

        </div>
        <div class="block chart avg_memory" id="avg_memory_chart">

        </div>
      </div>
      <div class="block core_chart_area">

      </div>
      <div class="block process_chart_area">
        <h4>Process Usage Chart <small class="process_names"></small></h4>
        <div class="block chart process_cpu" id="process_cpu_chart">

        </div>
        <div class="block chart process_memory" id="process_memory_chart">

        </div>
        <div class="block chart process_thread" id="process_thread_chart">

        </div>
      </div>
    </div>

    <div class="dashboard_table_area">
      <h4>Process Information Table</h4>
      <div class ="block process_info_table_area">
      <div id="process_info_table" class="xtable" style="margin-top: 7px;">
        <table class="table simple headline nowrap">
          <thead>
            <tr>
              <th>Name</th>
              <th>ID</th>
              <th>CPU</th>
              <th>Memory</th>
              <th>Thread Count</th>
            </tr>
          </thead>
          <tbody></tbody>
        </table>
      </div>
      <script type="text/template" id="tpl_row">
        <! if(selectedProcessRowName == process_id) { !>
          <tr class="selected">
        <! } else { !>
          <tr>
        <! } !>
          <td>
            <!= name !>
          </td>
          <td>
            <!= process_id !>
          </td>
          <td>
            <!= process_cpu !> %
          </td>
          <td>
            <!= process_memory !> MB
          </td>
          <td>
            <!= process_thread !>
          </td>
        </tr>
      </script>
      <script id="tpl_none" type="text/template">
        <tr>
          <td colspan="5" class="none" align="center">Data does not exist.</td>
        </tr>
      </script>
      </div>
    </div>

  </div>
</body>

</html>
